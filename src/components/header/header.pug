mixin header(type)
  .header-wrapper(class = `${type === 'home' ? 'home' : 'not-home'}`)
    header.header(style = type === 'home' ? 'background: rgba(20, 37, 53, 0.9)' : 'background: #142535')
      .header__wrapper.wrapper
        nav.navbar.navbar-expand-lg.navbar-dark
          .container.navbar-container
            a.navbar-brand(href='index.html')
              img.img-fluid.desktop(src=require("@/assets/images/static/royal-king-logo.png") alt='image')
              img.img-fluid.mobile(src=require("@/assets/images/static/royal-king-logo.png") alt='image')
            button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbar-content', aria-controls='navbar-content', aria-expanded='false', aria-label='Toggle navigation')
              //- span.navbar-toggler-icon
              img(src=require("@/assets/images/icons/menu.png") alt='image')
            #navbar-content.collapse.navbar-collapse
              .navbar-content-wrapper
                .contact-info
                  .contact-info__item
                    a(href='mailto:info@xxxxx.com')
                      i.bi.bi-envelope
                      span info@xxxxx.com
                  .contact-info__item
                    a(href='tel:+1234567890')
                      i.bi.bi-phone
                      span +1 234 567 890
                  .contact-info__item.language
                    //- img(src=require("@/assets/images/icons/myanmar.png") alt='image')
                    .dropdown
                      button.btn.btn-secondary.dropdown-toggle(type='button', data-bs-toggle='dropdown', aria-expanded='false')
                        img(src=require("@/assets/images/icons/myanmar.png") alt='image')
                      ul.dropdown-menu
                        li
                          a.dropdown-item.active(href='#')
                            img(src=require("@/assets/images/icons/myanmar.png") alt='myanmar')
                            span Myanmar
                        li
                          a.dropdown-item(href='#')
                            img(src=require("@/assets/images/icons/english.png") alt='english')
                            span English

                #navbar-content-item
                  ul.navbar-nav.mr-auto.mb-2.mb-lg-0
                    li.nav-item
                      a.nav-link.active(aria-current='page' href='index.html') Home
                    li.nav-item.dropdown.dropdown-mega.position-static
                      a.nav-link.dropdown-toggle(href='#' data-bs-toggle='dropdown' data-bs-auto-close='outside') For Sale
                      .dropdown-menu.shadow
                        .mega-content
                          .row
                            .col-12.col-lg-8
                              h5.title Property For Sale
                              .row.icon-row
                                .col-4.col-md-3
                                  a.icon-wrapper(href='#') 
                                    .icon
                                      img.img-fluid(src=require("@/assets/images/icons/condo-logo.svg") alt='image')
                                    span Condo
                                .col-4.col-md-3
                                  a.icon-wrapper(href='#') 
                                    .icon
                                      img.img-fluid(src=require("@/assets/images/icons/apartment-logo.svg") alt='image')
                                    span Apartment
                                .col-4.col-md-3 
                                  a.icon-wrapper(href='#') 
                                    .icon
                                      img.img-fluid(src=require("@/assets/images/icons/house-logo.svg") alt='image')
